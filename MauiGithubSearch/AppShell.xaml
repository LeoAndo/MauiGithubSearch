<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiGithubSearch.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pagination="clr-namespace:MauiGithubSearch.UI.Search.Pagination"
    xmlns:search="clr-namespace:MauiGithubSearch.UI.Search"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="non-compatible version"
        ContentTemplate="{DataTemplate search:SearchScreen}"
        Route="SearchScreen" />
    <ShellContent
        Title="Paging compatible version"
        ContentTemplate="{DataTemplate pagination:SearchPagingScreen}"
        Route="SearchPagingScreen" />

</Shell>

